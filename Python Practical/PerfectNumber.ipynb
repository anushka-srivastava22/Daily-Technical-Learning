   "source": [
    "def isPerfect(num):  \n",
    "    sum = 0  \n",
    "    for i in range(1, num):  \n",
    "        if num % i == 0:  \n",
    "            sum=sum+i  \n",
    "    return sum == num  \n",
    "\n",
    "n = (int)(input())\n",
    "\n",
    "if isPerfect(n):\n",
    "    print(n , \" is a Perfect Number\")\n",
    "else:\n",
    "    print(n, \" is not a Perfect Number\")"
   ]
