    "def isPalindromeString(s):\n",
    "    rev = ''.join(reversed(s))\n",
    "    if(s == rev):\n",
    "        return True\n",
    "    return False\n",
    "\n",
    "s = input()\n",
    "res = isPalindromeString(s)\n",
    "if(res):\n",
    "    print(\"Yes it is a palindrome string.\")\n",
    "else:\n",
    "    print(\"No it is not a palindrome string.\")"
   ]
